# Copy to `.env` and fill in as needed. `.env` is ignored by git.

# Provider settings (optional)
# OPENAI_API_KEY=sk-...
# OPENAI_BASE_URL=https://api.openai.com/v1
# OPENAI_MODEL=gpt-4o-mini

# Agent Maker runtime
# Privacy levels: off | standard | strict
AGENT_MAKER_PRIVACY=standard

# Enable/disable trace file output under runs/<id>/trace.jsonl
AGENT_MAKER_TRACE_ENABLED=true

# Redaction placeholder and max value length when not strict
AGENT_MAKER_REDACT_PLACEHOLDER=***
AGENT_MAKER_MAX_VALUE_LEN=2000

# Custom dotenv path (defaults to .env)
# AGENT_MAKER_DOTENV=.env

